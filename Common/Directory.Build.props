<Project>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NETCORE3_1')) == 'true'">
      <BaseOutputPath>bin\server-netcoreapp3.1\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-netcoreapp3.1\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\server-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('WasmCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\wasm-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
</Project>